@model IEnumerable<razorweb.ViewModels.ArticleVM>
@{
    ViewData["Title"] = "Danh sách bài viết";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h1>Danh sách bài viết</h1>

<table class="table">
    <thead>
        <tr>
            <th>ID</th>
            <th>Title</th>
            <th>Create At</th>
            <th>Content</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.Id</td>
                <td>@item.Title</td>
                <td>@item.CreateAt</td>
                <td>@item.Content</td>
            </tr>
        }
    </tbody>
</table>
